    "parcel-plugin-svelte": "^3.0.1",
    "postcss-preset-env": "^6.7.0",
    "svelte": "^3.6.1",
    "svelte-fragment": "^1.0.1",
    "file-saver": "^2.0.2",
    "html2canvas": "^1.0.0-rc.3",
    "postcss-nested": "^4.1.2"
  },
  "browserslist": [
    "last 1 chrome versions"
  ],
  "svelte": {
    "compilerOptions": {
      "css": false
    }
  }
